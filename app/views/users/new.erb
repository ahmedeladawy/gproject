<h1>Sign Up</h1>

<%= @user.errors.full_messages.join(', ') %>

<%= form_for @user do |f| %>
 <div class="">
   <%= f.label :First_name %>
   <%= f.text_field :first_name %>
 </div>
 <div class="">
   <%= f.label :Last_name %>
   <%= f.text_field  :last_name%>
 </div>
 <div class="">
   <%= f.label :Email%>
   <%= f.email_field :email %>
 </div>
 <div class="">
   <%= f.label :Password_Confirmation %>
   <%= f.password_field :password_confirmation %>
 </div>
  <%= f.submit 'Sign Up' %>
<% end %>
<p>Already have account? <%= link_to 'Sign In', new_session_path %></p>
